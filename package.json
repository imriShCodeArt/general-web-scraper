    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist",
    "recipe": "ts-node src/cli/recipe-cli.ts",
    "test:recipe": "ts-node src/test-recipe-system.ts",
    "test:backend": "ts-node src/test-backend-complete.ts"
  },
  "keywords": [
    "web-scraper",
    "ecommerce",
    "woocommerce",
    "csv",
    "universal"
