  "description": "Universal web scraper with deterministic CSV outputs and WooCommerce compatibility",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
