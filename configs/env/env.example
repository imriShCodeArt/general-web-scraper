# Web Scraper Environment Configuration

# Application
NODE_ENV=development
PORT=3000
HOSTNAME=localhost

# Database (if using external database)
# DATABASE_URL=postgresql://user:password@localhost:5432/webscraper

# Redis (if using Redis for job queue)
# REDIS_URL=redis://localhost:6379

# External Services
# WOOCOMMERCE_API_URL=https://your-store.com/wp-json/wc/v3
# WOOCOMMERCE_CONSUMER_KEY=your_consumer_key
# WOOCOMMERCE_CONSUMER_SECRET=your_consumer_secret

# Security
# JWT_SECRET=your_jwt_secret_here
# CORS_ORIGIN=http://localhost:3001

# Logging
LOG_LEVEL=info
LOG_PRETTY=true

# Scraping Configuration
# MAX_CONCURRENT_JOBS=5
# REQUEST_DELAY=1000
# USER_AGENT_ROTATION=true

# Storage
# STORAGE_PATH=./storage
# MAX_STORAGE_SIZE=1GB
# STORAGE_CLEANUP_INTERVAL=3600000
